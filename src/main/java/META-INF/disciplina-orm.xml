<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm http://xmlns.jcp.org/xml/ns/persistence/orm_2_1.xsd">
	<persistence-unit-metadata>
		<persistence-unit-defaults>
			<access>FIELD</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<package>digital-valley-m2c</package>
	<entity class="model.Disciplina" name="Disciplina" access="FIELD" >
		<table name="disciplina" />
		<attributes>
			<id name="id">
				<column name="id_disciplina" unique="true" nullable="false" />
				<generated-value strategy="SEQUENCE" generator="DISCIPLINA_ID_GENERATOR" />
				<sequence-generator name="DISCIPLINA_ID_GENERATOR" allocation-size="1" sequence-name="disciplina_id_disciplina_seq" />
			</id>

			<basic name="nome" optional="false">
				<column name="nome" />
			</basic>
		
			
		</attributes>	
	</entity>
</entity-mappings>
